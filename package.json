{
  "name": "angular-ngrx-storybook-starter-kit",
  "version": "0.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/wahid-racheh/angular-ngrx-storybook-starter-kit"
  },
  "author": "Racheh",
  "contributors": [
    "Racheh Wahid <racheh.wahid@gmail.com>"
  ],
  "private": true,
  "scripts": {
    "setup": "node ./scripts/verify && npm i && npm run build",
    "start": "node_modules/.bin/ng serve",
    "clean:dist": "rimraf dist",
    "clean:coverage": "rimraf coverage",
    "build:prod": "node_modules/.bin/ng build --aot --prod",
    "build": "npm run clean:dist && npm run build:prod",
    "jest:test": "node_modules/.bin/jest -c jest-config/jest.config.js -u --verbose",
    "jest:debug": "node_modules/.bin/jest -c jest-config/jest.config.js -u --runInBand",
    "jest:coverage": "node_modules/.bin/jest -c jest-config/jest.config.js -u --coverage",
    "jest:clear-cache": "node_modules/.bin/jest --clearCache",
    "test": "npm run jest:clear-cache && npm run jest:test",
    "test:debug": "npm run jest:clear-cache && npm run jest:debug",
    "test:coverage": "npm run jest:clear-cache && npm run clean:coverage && npm run jest:coverage",
    "pretty-quick": "npx pretty-quick --staged",
    "pre-lint": "node_modules/.bin/tsc --noEmit --pretty --project src/tsconfig.app.json",
    "post-lint": "node_modules/.bin/ng lint --format stylish",
    "lint": "npm run pretty-quick && npm run pre-lint && npm run post-lint",
    "e2e": "node_modules/.bin/ng e2e",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "semantic": "npx semantic-release --dry-run",
    "doc": "compodoc -p ./src/tsconfig.app.json -s -o",
    "sonar-scanner": "node_modules/.bin/sonar-scanner",
    "sonar": "npm run sonar-scanner"
  },
  "dependencies": {
    "@angular/animations": "~9.0.1",
    "@angular/cdk": "^9.0.0",
    "@angular/common": "~9.0.1",
    "@angular/compiler": "~9.0.1",
    "@angular/core": "~9.0.1",
    "@angular/forms": "~9.0.1",
    "@angular/material": "^9.0.0",
    "@angular/material-moment-adapter": "^9.0.0",
    "@angular/platform-browser": "~9.0.1",
    "@angular/platform-browser-dynamic": "~9.0.1",
    "@angular/router": "~9.0.1",
    "@compodoc/compodoc": "^1.1.11",
    "@fortawesome/fontawesome-free": "^5.12.0",
    "@ngrx/effects": "^8.6.0",
    "@ngrx/router-store": "^8.6.0",
    "@ngrx/store": "^8.6.0",
    "@ngrx/store-devtools": "^8.6.0",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^4.0.0",
    "bootstrap": "^4.4.1",
    "core-js": "^2.5.4",
    "hammerjs": "^2.0.8",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "ngrx-store-freeze": "^0.2.4",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.2",
    "@angular/cli": "~9.0.2",
    "@angular/compiler-cli": "~9.0.1",
    "@angular/language-service": "~9.0.1",
    "@babel/core": "^7.7.7",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-typescript": "^7.8.3",
    "@commitlint/cli": "^8.3.4",
    "@commitlint/config-conventional": "^8.3.4",
    "@semantic-release/changelog": "^3.0.6",
    "@semantic-release/commit-analyzer": "^6.3.3",
    "@semantic-release/git": "^7.0.18",
    "@semantic-release/gitlab": "^4.1.0",
    "@semantic-release/npm": "^5.3.5",
    "@semantic-release/release-notes-generator": "^7.3.5",
    "@storybook/addon-actions": "^5.3.13",
    "@storybook/addon-backgrounds": "^5.3.13",
    "@storybook/addon-jest": "^5.3.13",
    "@storybook/addon-knobs": "^5.3.13",
    "@storybook/addon-links": "^5.3.13",
    "@storybook/addon-notes": "^5.3.13",
    "@storybook/addon-options": "^5.3.13",
    "@storybook/addon-storyshots": "^5.3.13",
    "@storybook/addons": "^5.3.13",
    "@storybook/angular": "^5.3.13",
    "@testing-library/angular": "^8.2.0",
    "@testing-library/jest-dom": "^5.1.1",
    "@types/jest": "^24.9.1",
    "@types/moment": "^2.13.0",
    "@types/node": "~8.9.4",
    "@types/storybook__addon-storyshots": "^5.3.1",
    "@types/webpack-env": "^1.14.1",
    "babel-loader": "^8.0.6",
    "babel-plugin-require-context-hook": "^1.0.0",
    "codelyzer": "~4.5.0",
    "commitlint": "^8.2.0",
    "husky": "^3.1.0",
    "jasmine-marbles": "^0.6.0",
    "jest": "^24.9.0",
    "jest-canvas-mock": "^2.1.0",
    "jest-preset-angular": "^7.1.1",
    "jest-sonar-reporter": "^2.0.0",
    "lint-staged": "^9.5.0",
    "node-sass": "^4.13.0",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "rimraf": "^3.0.0",
    "sonarqube-scanner": "^2.5.0",
    "ts-node": "~7.0.0",
    "tsconfig-paths-jest": "0.0.1",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "tslint": "^5.11.0",
    "tslint-angular": "^3.0.2",
    "tslint-config-airbnb": "^5.11.2",
    "tslint-config-prettier": "^1.18.0",
    "tslint-sonarts": "^1.9.0",
    "typescript": "~3.7.5"
  },
  "jestSonar": {
    "reportPath": "coverage",
    "reportFile": "sonar-reporter.xml",
    "indent": 4
  }
}
